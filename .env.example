# Environment variables for HuggingFace Spaces deployment
# Copy to .env and set your values

# Alberto API endpoint (when running bridge server separately)
ALBERTO_API_URL=http://localhost:8080

# API key for authentication (if required)
ALBERTO_API_KEY=

# Request timeout in seconds
ALBERTO_TIMEOUT=60

# Web interface configuration
HOST=0.0.0.0
PORT=7860

# Debug mode (true in development, false in production)
DEBUG=false

# Log level (debug, info, warning, error)
LOG_LEVEL=info